{
  "id": "connecttomailchimp-demo",
  "version": "1.0.7",
  "name": "Connect to Mailchimp",
  "icon": "src/views/icon.png",
  "permissions": [
    {
      "permission": "checkout_session_read",
      "purpose": "Read Checkout Session data in webhook events"
    }
  ],
  "connect_permissions": null,
  "ui_extension": {
    "views": [
      {
        "viewport": "stripe.dashboard.home.overview",
        "component": "Home"
      }
    ],
    "content_security_policy": {
      "connect-src": [
        "https://api.connectto.app/api/"
      ],
      "image-src": [],
      "purpose": "This urls allow Connect To Mailchimp to call backend server for user management and mailchimp integration."
    }
  },
  "distribution_type": "private"
}